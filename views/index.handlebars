<h1>BURGERS!</h1>

<button id="submit" type="submit">Add Burger</button>





<ul>
  {{#each burger}}
    {{#unless devoured}}
      {{> burger devoured=false}}
    {{/unless}}
  {{/each}}
</ul>

<form class="create-form">

  <div class="form-group">
    <label for="ca">What Kind of Burger do you Want?:</label>
    <input type="text" id="ca" name="name">
  </div>

  
</form>
